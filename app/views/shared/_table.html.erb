<% @reserves.each do |reserve| %>
      <tr> 
        <td><%= reserve.id %> </td>
        <td><%= reserve.book_id %> </td>
        <td><%= reserve.book.title %> </td>
        <td><%= reserve.book.author %> </td>
        <td><%= link_to 'Destroy reserve',
        delete_path(reserve.id), method: :delete, class: "btn btn-light" %></td>
      </tr> 
    <% end %>